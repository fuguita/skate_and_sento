<%= form_with url: search_path, method: :get do |f| %>
  <%= f.text_field :word, placeholder: "キーワード検索", class: "form-control col-12 mb-2" %>
    <%= f.select :model, options_for_select([['Park'],['銭湯']]), prompt: "選択してください" %>
      <%= f.submit '検索', class: "btn btn-sm btn-outline-secondary" %>
<% end %>